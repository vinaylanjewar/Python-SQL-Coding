## Method 1

n =int(input("Enter number: "))
count = 0

for i in range(1,n+1):
    if n%i==0:
        count+=1

if count==2:
    print("Prime")
else:
    print("Not Prime")

## Method 2

num = int(input('enter number'))
for i in range(2,num):
    if num%i ==0:
        print('number is not prime')
        break
else:
    print('number is prime')
